{
  "presets": [
    ["env", {
      "modules": false,
      "debug": false,
      "targets": {
        "browsers": ["last 2 versions"],
        "ie": ["11"],
        "node": "current"
      }
    }],
    "react",
  ],
  "plugins": [
    // Static Properties
    "transform-class-properties",
    // export * as
    "transform-export-extensions",
    // spread operator
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      // minification
      "presets": ["babili"],
      // only include helpers once
      "plugins": ["external-helpers"],
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
