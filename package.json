{
  "name": "componentry",
  "version": "1.0.0",
  "description": "Radical React Components",
  "main": "index.js",
  "engines": { "node": ">=6.9.5" },
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/componentry"
  },
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "sass": "node-sass src/styles/styles.scss dist/build/radical-ui.css",
    "svg": "svg-sprite --defs --defs-dest=dist/assets --defs-sprite=svg-defs.svg dist/assets/svgs/*.svg",
    "test": "BABEL_ENV=test mocha src/test/setup.js src/**/*.spec.js"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "508",
    "accessible",
    "bootstrap"
  ],
  "author": "Dan Hedgecock <hedgecock.d@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@dhedgecock/eloquence": "1.0.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "sinon": "^2.0.0",
    "svg-sprite": "^1.3.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
